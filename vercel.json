{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/api/all-pages/index.html"
    },
    {
      "src": "/api/all-pages/(.*)",
      "dest": "/api/all-pages/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
} 